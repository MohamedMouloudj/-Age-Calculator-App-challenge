{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "semi": "off",
    "linebreak-style": "off",
    "radix": "off",
    "no-use-before-define": "off",
    "no-unused-vars": "off",
    "no-else-return": "off",
    "prefer-const": "off",
    "max-len": "off"
  }
}
